<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pass.net4_proyecto_integrador">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logoapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logoapp_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Net4">
        <activity android:name=".SignUpActivity"
            android:theme="@style/Theme.Net4.NoActionBar"></activity>
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogInActivity"
            android:theme="@style/Theme.Net4.NoActionBar" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>